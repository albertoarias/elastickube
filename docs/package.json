{
  "name": "ElasticKubeDocs",
  "version": "1.0.0",
  "description": "ElasticKube Documentation",
  "watch": {
    "clean-dependencies": {
      "patterns": [
        "."
      ],
      "ignore": [
        "src/*",
        "k8s_theme/*"
      ],
      "extensions": "json"
    }
  },
  "scripts": {
    "start": "parallelshell 'webpack --progress --colors --watch' 'sphinx-autobuild -b dirhtml ./src ../src/build/documentation' 'npm-watch'",
    "clean-dependencies": "npm install && npm prune",
    "build": "webpack --progress --colors",
    "build:production": "webpack -p --progress --colors && sphinx-build -b dirhtml ./src ../src/build/documentation"
  },
  "devDependencies": {
    "angular": "^1.5.3",
    "angular-animate": "^1.5.3",
    "angular-aria": "^1.5.3",
    "angular-material": "^1.0.7",
    "angular-messages": "^1.5.3",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.15.0",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "normalize.css": "^4.1.1",
    "npm-watch": "^0.1.3",
    "parallelshell": "^2.0.0",
    "postcss-loader": "^0.8.2",
    "roboto-fontface": "^0.4.5",
    "script-loader": "^0.6.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.15",
    "webpack-shell-plugin": "^0.3.5"
  }
}
